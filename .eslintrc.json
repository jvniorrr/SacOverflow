{
	"extends": ["next/core-web-vitals"],
	"rules": {
		"no-unused-vars": "warn",
		"no-console": "warn",
		"no-unreachable": "warn",
		"consistent-return": "error",
		"curly": ["error", "all"],
		"default-case": "error",
		"default-case-last": "error",

		// meh this one below
		"eqeqeq": ["error", "always"],
		"prefer-const": "warn",
		"@typescript-eslint/explicit-module-boundary-types": "off",
		"@typescript-eslint/no-explicit-any": ["off"]
	}
}
